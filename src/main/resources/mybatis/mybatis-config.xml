<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <typeAliases>
        <typeAlias alias="Integer" type="java.lang.Integer"/>
        <typeAlias alias="Long" type="java.lang.Long"/>
        <typeAlias alias="HashMap" type="java.util.HashMap"/>
        <typeAlias alias="LinkedHashMap" type="java.util.LinkedHashMap"/>
        <typeAlias alias="ArrayList" type="java.util.ArrayList"/>
        <typeAlias alias="LinkedList" type="java.util.LinkedList"/>
        <typeAlias alias="Boolean" type="java.lang.Boolean"/>
        <typeAlias alias="String" type="java.lang.String"/>

        <typeAlias type="com.dzlin.oauth2.admin.entity.UserEntity" alias="User"/>
        <typeAlias type="com.dzlin.oauth2.admin.entity.AdminUserEntity" alias="AdminUser"/>
        <typeAlias type="com.dzlin.oauth2.admin.entity.AuthorityEntity" alias="Authority"/>
        <typeAlias type="com.dzlin.oauth2.admin.entity.OauthClientDetailsEntity" alias="OauthClientDetailsEntity"/>
    </typeAliases>

</configuration>